!URxvt*font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-1
!URxvt*boldFont: -*-terminus-medium-*-*-*-12-*-*-*-*-*-iso8859-1

! Colors

URxvt*background: #181818
URxvt*foreground: wheat
URxvt*cursorColor: #93a1a1

!! black dark/light
URxvt*color0: #121212
URxvt*color8: #2e3436

!! red dark/light
URxvt*color1: #a35b66
URxvt*color9: #ab6b74

!! green dark/light
URxvt*color2: #99ab6f
URxvt*color10: #acb972

!! yellow dark/light
URxvt*color3: #ca9733
URxvt*color11: #ccaa69

!! blue dark/light
URxvt*color4: #495d6e
URxvt*color12: #687987

!! magenta dark/light
URxvt*color5: #825969
URxvt*color13: #977381

!! cyan dark/light
URxvt*color6: #839191
URxvt*color14: #98a4a4

!! white dark/light
URxvt*color7: #e0e0e0
URxvtcolor15: #e5e5e5

! Font
!URxvt.font: xft:Monospace:pixelsize=12

! Open links in 
URxvt.perl-ext-common : default,matcher,clipboard-osc,tabbed
URxvt.urlLauncher     : google-chrome
URxvt.matcher.button  : 1
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0


!URxvt.keysym.Control-l: echo -ne '\\033c'\n

! transparent
!urxvt*depth: 32
URxvt*transparent: true
urxvt*shading: 20
urxvt*blurRadius: 10
!urxvt*tintColor: #ffffff

