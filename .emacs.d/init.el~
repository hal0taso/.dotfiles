(require 'jedi)

(pallet-mode t)
(require 'py-autopep8)

(add-hook 'python-mode-hook
	  '(lambda()
	     (define-key python-mode-map (kbd "C-c F") 'py-autopep8)
	     ; バッファ全体のコード整形
	     (define-key python-mode-map (kbd "C-c f") 'py-autopep8-region)))
	     ; 選択リジョン内のコード整形

(setq init-loader-show-log-after-init nil)
(init-loader-load "~/.emacs.d/inits")
